<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.jsh.model.po.Express" table="jsh_express" schema="jsh_erp">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="expressName">
            <column name="expressName" sql-type="varchar(128)" length="32" not-null="true"/>
        </property>
        <property name="expressCode">
            <column name="expressCode" sql-type="varchar(64)"/>
        </property>
        <property name="sortOrder">
            <column name="sortOrder" sql-type="int" not-null="true"/>
        </property>
        <property name="enabled">
            <column name="enabled" sql-type="bit(1)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>